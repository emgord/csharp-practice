@using System.Security.Principal
@using FoodFinder
@model FoodFinder.ViewModels.SearchViewModel

<iframe width="800"
        height="450"
        frameborder="0" style="border: 0"
        src= @Model.MapUrl allowfullscreen> 
</iframe>

@foreach (var place in Model.Places)
{
    <div>
        <a href=@Model.buildStopoverLink(@place.place_id)>@place.name</a>
    </div>
}